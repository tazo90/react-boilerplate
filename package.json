{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplate",
  "main": "src/client/index.tsx",
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@lingui/react": "^3.10.2",
    "antd": "^4.16.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.9.0",
    "react-query": "^3.18.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@cypress/webpack-preprocessor": "^5.9.1",
    "@lingui/cli": "^3.10.2",
    "@lingui/macro": "^3.10.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.23",
    "@types/lingui__core": "^3.0.0",
    "@types/lingui__macro": "^3.0.0",
    "@types/lingui__react": "^3.0.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.10",
    "@types/testing-library__cypress": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "cypress": "^7.6.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-webpack-plugin": "^2.5.4",
    "fibers": "^5.0.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "jest-extended": "^0.11.5",
    "jest-raw-loader": "^1.0.1",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^6.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "prettier-webpack-plugin": "^1.2.0",
    "react-refresh": "^0.10.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.2.2",
    "ts-jest": "^27.0.3",
    "type-fest": "^1.2.1",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss}\"",
    "eslint": "eslint src/**/*.{ts,tsx}",
    "i18n:add": "lingui add-locale",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile --typescript",
    "test": "jest --config config/jest.config.ts",
    "test:e2e": "cypress run --config-file config/cypress.config.json",
    "test:e2e:open": "cypress open --config-file config/cypress.config.json",
    "test:coverage": "jest --config config/jest.config.ts --coverage --watchAll=false",
    "test:coverage:ci": "jest --config config/jest.config.ts --ci --coverage --watchAll=false && cat ./coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tazo90/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "tailwind",
    "antd",
    "styled-components"
  ],
  "author": "Mariusz Winnik",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tazo90/react-boilerplate/issues"
  },
  "homepage": "https://github.com/tazo90/react-boilerplate#readme"
}
